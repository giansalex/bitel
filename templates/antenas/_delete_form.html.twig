<form method="post" action="{{ path('antenas_delete', {'id': antena.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ antena.id) }}">
    <button class="btn">Delete</button>
</form>
